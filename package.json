{
  "name": "@smorken/aws-cdk",
  "version": "2.0.0",
  "license": "MIT",
  "author": "Scott Morken <scott.morken@phoenixcollege.edu>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ss10sb/aws-cdk.git"
  },
  "main": "./lib/cjs/index.js",
  "types": "./lib/types/index.d.ts",
  "scripts": {
    "build": "rm -rf lib/* && tsc",
    "lint": "eslint .",
    "dpdm": "dpdm --tree --circular ./src",
    "prepublish": "npm run build",
    "test": "jest",
    "watch": "tsc -w"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@aws-sdk/client-ecr": "^3.51.0",
    "@aws-sdk/client-ssm": "^3.51.0",
    "aws-cdk-lib": "^2.12.0",
    "constructs": "^10.0.0",
    "deepmerge": "^4.2.2",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.17",
    "@typescript-eslint/parser": "^5.12.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "aws-cdk-lib": "^2.12.0",
    "aws-sdk-client-mock": "^0.6.0",
    "constructs": "^10.0.0",
    "eslint": "^8.9.0",
    "jest": "^27.5.1",
    "dpdm": "^3.8.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.12.0",
    "constructs": "^10.0.0"
  },
  "exports": {
    ".": {
      "import": "./lib/cjs/index.js"
    },
    "./core": {
      "import": "./lib/cjs/core/index.js"
    },
    "./utils": {
      "import": "./lib/cjs/utils/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "lib/index.d.ts": [
        "lib/index.d.ts"
      ],
      "*": [
        "lib/*",
        "lib/*/index.d.ts"
      ]
    }
  }
}
