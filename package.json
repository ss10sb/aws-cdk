    "aws-cdk-lib": "2.190.0",
    "constructs": "^10.4.2",
    "lodash": "^4.17.21",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.14.1",
    "@typescript-eslint/parser": "^8.31.0",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
